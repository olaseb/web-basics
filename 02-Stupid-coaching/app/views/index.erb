<h1><%= @coach_message if @coach_message%></h1>
<h2><%= @coach_label2 if @coach_label2%></h2>


<form action="/coach" method="post">
	<form>
		<label for="user_message">What is your year of birth ?</label>
		<select name="years" id="user_message">
			<option value="">Years</option>
			<option value="1980">1980</option>
			<option value="1981">1981</option>
			<option value="1982">1982</option>
			<option value="1983">1983</option>
			<option value="1984">1984</option>
			<option value="1985">1985</option>
			<option value="1986">1986</option>
			<option value="1987">1987</option>
			<option value="1988">1988</option>
		</select>

		<label for="user_message">This is your year ? (yes, no)</label>
		<input type="text" name="verification" id="user_message">
		<input type="submit" value="Valid!">
	</form>
</form>

<br>	
	
